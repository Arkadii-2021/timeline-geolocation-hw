{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-cond-assign": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}